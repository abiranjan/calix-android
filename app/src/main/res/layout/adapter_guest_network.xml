<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/LayoutWidMatch_HeiWrap">

    <android.support.v7.widget.CardView
        style="@style/LayoutWidMatch_HeiWrap"
        android:layout_marginBottom="@dimen/size2"
        android:layout_marginLeft="@dimen/size5"
        android:layout_marginRight="@dimen/size5"
        android:layout_marginTop="@dimen/size4"
        android:padding="@dimen/size5">

        <RelativeLayout
            style="@style/LayoutWrap">
        <RelativeLayout
            android:id="@+id/adapter_guest_network_edit_lay"
            style="@style/LayoutWidMatch_HeiWrap">

            <LinearLayout
                style="@style/LayoutWrap"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:layout_toStartOf="@+id/adapter_guest_network_arrow_right_img"
                android:orientation="vertical"
                android:paddingBottom="@dimen/size10"
                android:paddingEnd="0dp"
                android:paddingStart="@dimen/size13"
                android:paddingTop="@dimen/size10">

                <TextView
                    android:id="@+id/event_name_txt"
                    style="@style/TextRobotoMedium"
                    android:layout_width="wrap_content"
                    android:gravity="start"
                    android:textColor="@color/black"
                    android:textSize="@dimen/size13" />

                <TextView
                    android:id="@+id/event_time_txt"
                    style="@style/TextRobotoRegular"
                    android:layout_width="wrap_content"
                    android:gravity="start"
                    android:paddingTop="@dimen/size2"
                    android:textColor="@color/green"
                    android:textSize="@dimen/size11" />
            </LinearLayout>

            <ImageView
                android:id="@+id/adapter_guest_network_arrow_right_img"
                android:layout_width="@dimen/size27"
                android:layout_height="@dimen/size27"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/size5"
                android:adjustViewBounds="true"
                android:contentDescription="@string/app_name"
                android:padding="@dimen/size5"
                android:src="@drawable/ic_guest_up_arrow" />
        </RelativeLayout>

        <RelativeLayout
            style="@style/LayoutWidMatch_HeiWrap"
            android:id="@+id/collapsedView"
            android:layout_below="@id/adapter_guest_network_edit_lay">

            <RelativeLayout
                style="@style/LayoutWidMatch_HeiWrap"
                android:id="@+id/dateValuesLay">

                <ImageView
                android:layout_width="@dimen/size50"
                android:layout_height="@dimen/size50"
                android:adjustViewBounds="true"
                    android:id="@+id/imageIcon"
                android:contentDescription="@string/app_name"
                android:paddingBottom="@dimen/size10"
                android:paddingEnd="@dimen/size10"
                android:paddingStart="0dp"
                android:paddingTop="@dimen/size10"
                android:src="@drawable/ic_calendar" />

                <TextView
                    android:id="@+id/start_date_txt"
                    android:text="@string/start_date"
                    android:layout_toEndOf="@+id/imageIcon"
                    style="@style/TextRobotoMedium"
                    android:paddingTop="@dimen/size15"
                    android:layout_width="wrap_content"
                    android:gravity="start"
                    android:textColor="@color/black"
                    android:textSize="@dimen/size11" />

                <TextView
                    android:id="@+id/end_date_txt"
                    android:text="@string/end_date"
                    android:layout_marginEnd="@dimen/size20"
                    android:layout_alignParentEnd="true"
                    style="@style/TextRobotoMedium"
                    android:paddingTop="@dimen/size15"
                    android:layout_width="wrap_content"
                    android:gravity="start"
                    android:textColor="@color/black"
                    android:textSize="@dimen/size11" />

            </RelativeLayout>

            <RelativeLayout
                style="@style/LayoutWidMatch_HeiWrap"
                android:layout_below="@+id/dateValuesLay"
                android:id="@+id/labelValuesLay">

                <ImageView
                    android:layout_width="@dimen/size50"
                    android:layout_height="@dimen/size50"
                    android:adjustViewBounds="true"
                    android:id="@+id/emptyImage"
                    android:contentDescription="@string/app_name"
                    android:paddingBottom="@dimen/size10"
                    android:paddingEnd="@dimen/size10"
                    android:paddingStart="0dp"
                    android:paddingTop="@dimen/size10"
                    android:visibility="invisible"
                    android:src="@drawable/ic_calendar" />

                <TextView
                    android:text="@string/start"
                    android:layout_toEndOf="@+id/emptyImage"
                    style="@style/TextRobotoMedium"
                    android:paddingTop="@dimen/size15"
                    android:layout_width="wrap_content"
                    android:gravity="start"
                    android:textColor="@color/blue"
                    android:textSize="@dimen/size11" />

                <TextView
                    android:text="@string/end"
                    android:layout_marginEnd="@dimen/size20"
                    android:layout_alignParentEnd="true"
                    style="@style/TextRobotoMedium"
                    android:paddingTop="@dimen/size15"
                    android:layout_width="wrap_content"
                    android:gravity="start"
                    android:textColor="@color/blue"
                    android:textSize="@dimen/size11" />

            </RelativeLayout>

            <RelativeLayout
                style="@style/LayoutWidMatch_HeiWrap"
                android:layout_below="@+id/labelValuesLay"
                android:id="@+id/timeValuesLay">

                <ImageView
                    android:layout_width="@dimen/size50"
                    android:layout_height="@dimen/size50"
                    android:adjustViewBounds="true"
                    android:id="@+id/imageIconTime"
                    android:contentDescription="@string/app_name"
                    android:paddingBottom="@dimen/size10"
                    android:paddingEnd="@dimen/size10"
                    android:paddingStart="0dp"
                    android:paddingTop="@dimen/size10"
                    android:src="@drawable/ic_guest_time" />

                <TextView
                    android:id="@+id/start_time_txt"
                    android:text="@string/start_date"
                    android:layout_toEndOf="@+id/imageIconTime"
                    style="@style/TextRobotoMedium"
                    android:paddingTop="@dimen/size15"
                    android:layout_width="wrap_content"
                    android:gravity="start"
                    android:textColor="@color/black"
                    android:textSize="@dimen/size11" />

                <TextView
                    android:id="@+id/end_time_txt"
                    android:text="@string/end_date"
                    android:layout_marginEnd="@dimen/size20"
                    android:layout_alignParentEnd="true"
                    style="@style/TextRobotoMedium"
                    android:paddingTop="@dimen/size15"
                    android:layout_width="wrap_content"
                    android:gravity="start"
                    android:textColor="@color/black"
                    android:textSize="@dimen/size11" />

            </RelativeLayout>

        </RelativeLayout>

        </RelativeLayout>
    </android.support.v7.widget.CardView>

</RelativeLayout>